<controls:ChildWindow x:Class="WebGraphs.LookupIsomorphismClassWindow"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           Width="400" Height="127" 
           Title="input board">
    <Grid x:Name="LayoutRoot" Margin="2">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="10,15,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="358" Name="_termBox" TextChanged="_termBox_TextChanged"/>
        <TextBlock Grid.Row="1" HorizontalAlignment="Center" Name="_isomorphismClass"></TextBlock>
    </Grid>
</controls:ChildWindow>